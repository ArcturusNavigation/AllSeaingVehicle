<launch>

	<!-- vim: set ft=xml noet : -->
	<node pkg="robot_localization" type="ekf_localization_node" name="ekf_local" clear_params="true">
		<rosparam command="load" file="$(find robot_localization)/params/ekf.yaml" />
	</node>

	<node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform_node" clear_params="true">
		<rosparam command="load" file="$(find robot_localization)/params/navsat_transform.yaml" />
		<remap from="/imu/data" to="/mavros/data/imu" />
		<remap from="/gps/fix" to="/mavros/global_position/global" />
	</node>

</launch>
